{
  "name": "imagizer",
  "version": "0.2.0",
  "description": "JavaScript library for image manipulation. Supports crop, resize, layers, blending modes, transform and color effects.",
  "main": "dist/imagizer.node.js",
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "canvas": "^1.6.5",
    "extend": "^3.0.0"
  },
  "scripts": {
    "build_prod_frontend": "webpack --config ./webpack.frontend.prod.config.js",
    "build_prod_backend": "webpack --config ./webpack.backend.prod.config.js",
    "build_dev_backend": "webpack --config ./webpack.backend.dev.config.js",
    "build_dev_frontend": "webpack --config ./webpack.frontend.dev.config.js",
    "watch": "webpack --progress --colors --watch --config ./webpack.dev.config.js",
    "test": "karma start --reporters html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/houdini22/imagizer.git"
  },
  "keywords": [
    "image",
    "graphic",
    "layer",
    "color",
    "transform",
    "effect",
    "crop",
    "resize"
  ],
  "homepage": "https://github.com/houdini22/imagizer",
  "directories": {
    "test": "test"
  }
}
